{
  "port": 4000,
  "hostname" : "ohnoitsyou.net",
  "remoteServer" : "monitor.ohnoitsyou.net",
  "services" : [
    {"name" : "nginx", "command" : "service nginx status"}
  ],
  "monitorMemory" : "true",
  "monitorTemp" : "true",
  "convertTemp" : "true",
  "cmds" : {
    "freeMemory" : "cat /proc/meminfo | grep 'MemFree' | awk '{print $2}'",
    "totalMemory": "cat /proc/meminfo | grep 'MemTotal'| awk '{print $2}'"
  }
}
